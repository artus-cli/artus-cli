{
  "extends": "@artus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "sourceMap": false,
    "inlineSourceMap": true,
    "noUnusedParameters": false,
    "noUnusedLocals":false,
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "ES2020",
    "outDir": "dist/esm",
    "resolveJsonModule": true
  },
  // "include": [
  //   "src/**/*.ts"
  // ],
  "ts-node": { "esm": true }
}
